<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.sea.shield.dispatch.mapper.ApiInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="pers.sea.shield.dispatch.pojo.entity.ApiInfo">
        <id column="uuid" property="uuid"/>
        <result column="item_code" property="itemCode"/>
        <result column="name" property="name"/>
        <result column="api_url" property="apiUrl"/>
        <result column="api_method" property="apiMethod"/>
        <result column="api_template" property="apiTemplate"/>
        <result column="result_type" property="resultType"/>
        <result column="result_template" property="resultTemplate"/>
        <result column="result_url" property="resultUrl"/>
        <result column="description" property="description"/>
        <result column="create_uid" property="createUid"/>
        <result column="create_time" property="createTime"/>
        <result column="modify_uid" property="modifyUid"/>
        <result column="modify_time" property="modifyTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        <if test="true">
            uuid, item_code, name, api_url, api_method, api_template, result_type, result_template,
            result_url, description, create_uid, create_time, modify_uid, modify_time
        </if>
    </sql>

</mapper>
