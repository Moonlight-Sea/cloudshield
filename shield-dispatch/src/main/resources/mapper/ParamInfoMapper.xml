<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.sea.shield.dispatch.mapper.ParamInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="pers.sea.shield.dispatch.pojo.entity.ParamInfo">
        <id column="uuid" property="uuid"/>
        <result column="api_uuid" property="apiUuid"/>
        <result column="code" property="code"/>
        <result column="name_cn" property="nameCn"/>
        <result column="mapping_code" property="mappingCode"/>
        <result column="data_type" property="dataType"/>
        <result column="must_input_check" property="mustInputCheck"/>
        <result column="description" property="description"/>
        <result column="create_uid" property="createUid"/>
        <result column="create_time" property="createTime"/>
        <result column="modify_uid" property="modifyUid"/>
        <result column="modify_time" property="modifyTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        <if test="true">
            uuid, api_uuid, code, name_cn, mapping_code, data_type, must_input_check, description, create_uid,
            create_time, modify_uid, modify_time
        </if>
    </sql>

</mapper>
